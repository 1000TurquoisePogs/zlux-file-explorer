{
  "identifier": "com.rs.zoe.zlux-file-explorer",
  "apiVersion": "1.0",
  "pluginVersion": "1.0",
  "pluginType": "application",
  "webContent": {
    "framework": "angular2",
    "launchDefinition": {
      "pluginShortNameKey": "Zlux File Explorer",
      "pluginShortNameDefault": "Zlux File Explorer",
      "imageSrc": "assets/icon.png"
    },
    "descriptionKey": "zlux-file-explorerDescription",
    "descriptionDefault": "zlux-file-explorer",
    "isSingleWindowApp": false,
    "defaultWindowStyle": {
      "width": 1100,
      "height": 682
    }
  },
  "configurationData": {
    "resources": {
      "zluxfileexplorer": {
        "variable": true,
        "locationType": "relative",
        "aggregationPolicy": "override"
      }
    }
  },
  "dataServices": []
}